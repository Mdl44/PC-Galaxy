document.addEventListener('DOMContentLoaded', function() {
    // VerificÄƒ dacÄƒ existÄƒ oferte active
    const timerElement = document.getElementById("timer");
    if (timerElement) {
        const dataFinalizare = timerElement.getAttribute("data-end-time");
        if (dataFinalizare) {
            const timer = new TimerOferte();
            timer.start(dataFinalizare);
        }
    }
});
</script>

<style>
@keyframes urgentPulse {
    0% { 
        transform: scale(1); 
        text-shadow: 0 0 5px #ff4757; 
    }
    100% { 
        transform: scale(1.1); 
        text-shadow: 0 0 15px #ff4757, 0 0 25px #ff4757; 
    }
}

@keyframes spin {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}

.timer.urgent {
    animation: urgentPulse 0.5s ease-in-out infinite alternate !important;
}

@media screen and (max-width: 480px) {
    .timer.urgent {
        font-size: 1.5rem !important;
    }
}

@media (prefers-reduced-motion: reduce) {
    .timer.urgent {
        animation: none !important;
        color: #ff4757 !important;
        font-weight: bold !important;
    }
}
</style>

</body>
</html>